{% extends 'base.html.twig' %} {% block title %}Symfony image parser{% endblock
%} {% block body %}
<style>
    .container-wrapper {
        margin: 1em auto;
        max-width: 800px;
        width: 95%;
        font: 18px/1.5 sans-serif;
    }

    .container-wrapper code {
        background: #f5f5f5;
        padding: 2px 6px;
    }

    .images, td {
        border: 1px solid;
    }

</style>

<div class="container-wrapper" style="display: flex; flex-direction: column; align-items: center;">
    <h1>Size of images: {{ size }} Mb</h1>

    <table class="images">
        <thead>
            <tr>
                Images
            </tr>
        </thead>
        <tbody>
            {% for imgs in imgs_all %}
                <tr>
                {% for img in imgs %}
                    <td><img src="{{img.src}}" style="height:100%; width:100%;"></td>
                {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
    </table>

</div>
{% endblock %}